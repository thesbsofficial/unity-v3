        return Response.json({
          error: `File ${photo.name} is not an image`
        }, { status: 400 });
      }
      const timestamp = Date.now();
      const filename = `${timestamp}-${photo.name}`;
      const r2Key = `temp/${sessionId}/${filename}`;
      await env.USER_UPLOADS.put(r2Key, photo.stream(), {
